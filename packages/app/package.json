{
  "name": "@slimfact/app",
  "version": "0.1.0",
  "private": true,
  "description": "SlimFact App",
  "author": "",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./fastify-ssr-plugin": {
      "import": "./dist/ssr/server/fastify-ssr-plugin.mjs"
    },
    "./hooks": {
      "import": "./dist/ssr/server/virtual_vitrify-hooks.mjs"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "vitrify dev",
    "dev:ssr": "vitrify dev -m ssr",
    "build": "run-s build:ssr minify:ssr:server minify:ssr:client",
    "build:csr": "vitrify build",
    "build:ssr": "vitrify build -m ssr",
    "build:ssg": "vitrify build -m ssg",
    "minify:csr": "vitrify minify ./dist/csr",
    "minify:ssr:server": "vitrify minify ./dist/ssr/server",
    "minify:ssr:client": "vitrify minify ./dist/ssr/client",
    "minify:ssg": "vitrify minify ./dist/static",
    "minify:fastify": "vitrify minify ./dist/server",
    "generate:types": "vue-tsc -p tsconfig.types.json",
    "test": "vitrify test",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "dependencies": {
    "@fastify/middie": "9.1.0",
    "@fastify/static": "9.0.0",
    "@slimfact/tools": "workspace:*",
    "cronstrue": "3.13.0",
    "uqr": "0.1.2"
  },
  "devDependencies": {
    "@iconify-json/fa6-brands": "1.2.6",
    "@iconify-json/fa6-solid": "1.2.4",
    "@iconify-json/flagpack": "1.2.7",
    "@iconify-json/mdi": "1.2.3",
    "@modular-api/fastify-checkout": "0.5.6",
    "@modular-api/quasar-components": "0.3.30",
    "@myriaddreamin/typst-ts-renderer": "0.7.0-rc2",
    "@myriaddreamin/typst-ts-web-compiler": "0.7.0-rc2",
    "@myriaddreamin/typst.ts": "0.7.0-rc2",
    "@myriaddreamin/typst.vue3": "0.7.0-rc2",
    "@pinia/colada": "0.21.5",
    "@quasar/extras": "1.17.0",
    "@quasar/quasar-ui-qcalendar": "4.1.2",
    "@simsustech/quasar-components": "0.12.0",
    "@slimfact/api": "link:../api",
    "@slimfact/shared": "link:../shared",
    "@trpc/client": "11.10.0",
    "@trpc/server": "11.10.0",
    "@typescript-eslint/eslint-plugin": "8.56.1",
    "@typescript-eslint/parser": "8.56.1",
    "@vitejs/plugin-basic-ssl": "^2.1.4",
    "@vitejs/plugin-vue": "6.0.4",
    "@vue/reactivity": "3.5.29",
    "@vue/runtime-core": "3.5.29",
    "@vue/runtime-dom": "3.5.29",
    "@vue/server-renderer": "3.5.29",
    "@vue/shared": "3.5.29",
    "@vueuse/core": "14.2.1",
    "abort-controller": "3.0.0",
    "beasties": "0.4.1",
    "date-holidays": "3.26.8",
    "eslint": "10.0.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-prettier-vue": "5.0.0",
    "eslint-plugin-vue": "10.8.0",
    "fastify": "5.7.4",
    "icon-gen": "5.0.0",
    "kysely": "0.28.11",
    "lionel-oauth-client": "npm:@stefanvh/lionel-oauth-client@^0.7.0",
    "node-fetch": "3.3.2",
    "npm-run-all": "4.1.5",
    "pinia": "3.0.4",
    "quasar": "2.18.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.1",
    "unocss": "66.6.2",
    "unocss-preset-quasar": "0.2.12",
