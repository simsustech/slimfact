{
  "name": "@slimfact/api",
  "version": "0.1.0",
  "private": true,
  "description": "SlimFact API",
  "author": "simsustech",
  "type": "module",
  "exports": {
    "./zod": {
      "types": "./src/zod/index.ts",
      "import": "./src/zod/index.ts"
    },
    "./trpc": {
      "types": "./src/trpc/index.ts"
    }
  },
  "files": [
    "dist/",
    "vitrify.config.ts"
  ],
  "scripts": {
    "build": "run-s build:api build:migrations seed:fake:generate",
    "build:api": "vitrify build -m fastify",
    "build:migrations": "tsc -p tsconfig.migrations.json",
    "dev": "vitrify dev -m fastify --port 3001",
    "docker-compose": "node ./docker-compose.dev.mjs",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "migrate:latest": "node ./dist/migrate.js",
    "seed:data": "node ./dist/seeds/data.js",
    "seed:fake": "node ./dist/seeds/fake.js",
    "seed:fake:generate": "node ./dist/seeds/fake/writeData.js",
    "seed:test": "node ./dist/seeds/test.js",
    "serve": "node ./dist/server/server.mjs",
    "start": "run-s migrate:latest serve",
    "start:dev-server": "vitrify dev -m fastify --port 3001"
  },
  "dependencies": {
    "@fastify/cors": "11.2.0",
    "@fastify/middie": "9.1.0",
    "@fastify/one-line-logger": "2.1.0",
    "@fastify/static": "9.0.0",
    "@modular-api/api": "0.6.11",
    "@modular-api/fastify-cart": "0.3.10",
    "@modular-api/fastify-checkout": "0.5.4",
    "@modular-api/fastify-oidc": "0.9.13",
    "@mollie/api-client": "4.4.0",
    "@slimfact/app": "workspace:*",
    "@slimfact/tools": "workspace:*",
    "@trpc/server": "11.9.0",
    "@vitrify/tools": "0.4.4",
    "axios": "1.13.4",
    "bcryptjs": "3.0.3",
    "compress-tag": "3.0.0",
    "docker-compose": "1.3.1",
    "fastify": "5.7.2",
    "handlebars": "4.7.8",
    "json-2-csv": "5.5.10",
    "kysely": "0.28.10",
    "npm-run-all": "4.1.5",
    "otplib": "12.0.0",
    "pg": "8.17.2",
    "pg-boss": "12.7.2",
    "sharp": "0.34.5",
    "svgo": "4.0.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@faker-js/faker": "10.2.0",
    "@iconify-json/fa6-brands": "1.2.6",
    "@iconify-json/fa6-solid": "1.2.4",
    "@iconify-json/flagpack": "1.2.7",
    "@iconify-json/mdi": "1.2.3",
    "@playwright/test": "1.58.0",
    "@types/bcryptjs": "3.0.0",
    "@types/oidc-provider": "9.5.0",
    "@types/pg": "8.16.0",
    "@vitejs/plugin-basic-ssl": "^2.1.4",
    "@vitejs/plugin-vue": "6.0.3",
    "beasties": "0.4.1",
    "date-fns": "4.1.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-vue": "10.7.0",
    "kysely-codegen": "0.19.0",
    "nodeunit": "0.11.3",
    "oidc-provider": "9.6.0",
    "quasar": "2.18.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "unocss": "66.6.0",
    "unocss-preset-quasar": "0.2.12",
    "vite": "7.3.1",
    "vitrify": "0.25.7",
    "vue": "3.5.27",
    "vue-eslint-parser": "10.2.0",
    "vue-router": "5.0.1"
  },
  "dependenciesMeta": {
    "@modular-api/fastify-oidc": {
      "injected": true
    }
  },
  "productName": "SlimFact API"
}
