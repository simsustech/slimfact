{
  "private": true,
  "name": "@slimfact/api",
  "productName": "SlimFact API",
  "version": "0.1.0",
  "description": "SlimFact API",
  "author": "simsustech",
  "type": "module",
  "exports": {
    "./zod": {
      "types": "./src/zod/index.ts",
      "import": "./src/zod/index.ts"
    },
    "./trpc": {
      "types": "./src/trpc/index.ts"
    }
  },
  "files": [
    "dist/",
    "vitrify.config.ts"
  ],
  "scripts": {
    "serve": "node ./dist/server/server.mjs",
    "dev": "run-s docker-compose start:dev-server",
    "build:migrations": "tsc -p tsconfig.migrations.json",
    "build:api": "vitrify build -m fastify",
    "build": "run-s build:api build:migrations seed:fake:generate",
    "test": "vitrify test",
    "test:e2e": "playwright test",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "migrate:latest": "node ./dist/migrate.js",
    "seed:fake:generate": "node ./dist/seeds/fake/writeData.js",
    "seed:fake": "node ./dist/seeds/fake.js",
    "seed:test": "node ./dist/seeds/test.js",
    "seed:data": "node ./dist/seeds/data.js",
    "start": "run-s migrate:latest serve",
    "start:dev-server": "vitrify dev -m fastify --port 3001",
    "docker-compose": "node ./docker-compose.dev.mjs"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/middie": "9.0.3",
    "@fastify/one-line-logger": "^2.0.2",
    "@fastify/static": "8.3.0",
    "@modular-api/api": "^0.6.11",
    "@modular-api/fastify-cart": "^0.3.10",
    "@modular-api/fastify-checkout": "^0.5.4",
    "@modular-api/fastify-oidc": "^0.9.13",
    "@mollie/api-client": "^4.3.3",
    "@orpc/server": "^1.10.0",
    "@slimfact/app": "workspace:*",
    "@slimfact/tools": "workspace:*",
    "@trpc/server": "^11.6.0",
    "@vitrify/tools": "^0.4.3",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "compress-tag": "^3.0.0",
    "docker-compose": "^1.3.0",
    "fastify": "^5.6.1",
    "handlebars": "^4.7.8",
    "json-2-csv": "^5.5.9",
    "kysely": "^0.28.8",
    "npm-run-all": "^4.1.5",
    "otplib": "^12.0.1",
    "pg": "^8.16.3",
    "pg-boss": "^11.1.1",
    "sharp": "^0.34.4",
    "svgo": "^4.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@iconify-json/fa6-brands": "^1.2.6",
    "@iconify-json/fa6-solid": "^1.2.4",
    "@iconify-json/flagpack": "^1.2.6",
    "@iconify-json/mdi": "^1.2.3",
    "@playwright/test": "1.56.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/oidc-provider": "^9.5.0",
    "@types/pg": "^8.15.5",
    "@vitejs/plugin-vue": "6.0.1",
    "beasties": "^0.3.5",
    "date-fns": "^4.1.0",
    "devcert": "1.2.2",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "10.5.1",
    "kysely-codegen": "^0.19.0",
    "nodeunit": "^0.11.3",
    "oidc-provider": "^9.5.1",
    "quasar": "2.18.5",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.2",
    "unocss": "^66.5.4",
    "unocss-preset-quasar": "^0.2.10",
    "vite": "^7.1.11",
    "vitrify": "^0.25.3",
    "vue": "3.5.22",
    "vue-eslint-parser": "^10.2.0",
    "vue-router": "4.6.3"
  },
  "dependenciesMeta": {
    "@modular-api/fastify-oidc": {
      "injected": true
    }
  }
}
